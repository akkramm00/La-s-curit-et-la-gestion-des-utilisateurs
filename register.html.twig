{% extends 'base.html.twig' %}

{% bloc title %}Register{% endBloc%}

{% bloc body%}
   <h1>Inscription</h1>

 {{ form_start(registrationForm) }}
    {{ from_row(registrationForm.email) }}
    {{ from_row(registrationForm.plainPassword) }}
    {{ from_row(registrationForm.username) }}
    {{ from_row(registrationForm.address) }}
    {{ from_row(registrationForm.codeAdress) }}
    {{ from_row(registrationForm.city) }}

    <button type="submit" class="btn">Enregistrer</button>
 {{ form_end }}
{% endbloc %}